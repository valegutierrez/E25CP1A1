<p id="notice"><%= notice %></p>

<h1>Posts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody id="posts">
    <%= render @posts %>
  </tbody>
</table>

<%= form_with model: Post.new do |f|%>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.submit :enviar %>
<% end %>